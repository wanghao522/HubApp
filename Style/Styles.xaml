                    </ControlTemplate>
                </ToggleButton.Template>
                <ToggleButton.Content>
                    <StackPanel Orientation="Horizontal" Height="36" >
                        <TextBlock Text="{Binding Index}" Foreground="Black" Style="{StaticResource smallText}"/>
                        <TextBlock Text="{Binding LineName}" Foreground="Black" Width="300" TextTrimming="WordEllipsis" Style="{StaticResource smallText}"/>
                        <TextBlock Text="{Binding LineFares}" Foreground="OrangeRed" Width="120" TextAlignment="Right" Style="{StaticResource smallText}"/>
                    </StackPanel>
                </ToggleButton.Content>
            </ToggleButton>
            <Border Padding="40,5,0,0">
                <StackPanel Visibility="{Binding IsChecked, ElementName=checkBox, Converter={StaticResource booleanToVisibilityConverter}}">
                    <TextBlock Text="{Binding LineName}" Foreground="Black" Padding="0"/>
                    <StackPanel Orientation="Horizontal" >
                        <TextBlock Text="首班车：" Foreground="Gray"/>
                        <TextBlock Text="{Binding FirstBus}" Style="{StaticResource smallText}" Foreground="OrangeRed"/>
                        <TextBlock Text="末班车：" Foreground="Gray" Padding="20,0,0,0"/>
                        <TextBlock Text="{Binding LineIntervalTime}"  Style="{StaticResource smallText}" Foreground="OrangeRed"/>
                    </StackPanel>
                    --><!--<StackPanel Orientation="Horizontal">
                        <TextBlock Text="间隔时间：" Foreground="Gray"/>
                        <TextBlock Text="{Binding LineIntervalTime}" Style="{StaticResource smallText}" Foreground="OrangeRed"/>
                    </StackPanel>--><!--
                    <TextBlock Text="{Binding LineStops,StringFormat='停靠车站：{0}'}"  TextWrapping="Wrap" Style="{StaticResource smallText}" Foreground="Gray"/>
                </StackPanel >
            </Border>
        </StackPanel>
    </DataTemplate>
